{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-js": "^9.1.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.1",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-emotion": "^8.0.12",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "color": "^2.0.1",
    "css-loader": "0.28.7",
    "dompurify": "^2.0.7",
    "dotenv": "4.0.0",
    "emotion": "^8.0.12",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.13",
    "lodash.assignin": "^4.2.0",
    "node-sass": "^4.7.2",
    "normalizr": "^3.2.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-detect-offline": "^1.0.6",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "^0.0.3",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-swipeable-views": "^0.12.12",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6",
    "shave": "^2.1.3",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "timeago.js": "^3.0.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "proxy": "http://localhost:3030",
  "scripts": {
    "start": "NODE_PATH=src NODE_ENV=development node scripts/start.js",
    "pre-build": "rm -rf ../server/build/client/*",
    "build": "NODE_PATH=src NODE_ENV=production node scripts/build.js",
    "post-build": "cd build && cp -rf . ../../server/build/client/",
    "production": "yarn pre-build && yarn build && yarn post-build",
    "test": "NODE_PATH=src NODE_ENV=test node scripts/test.js --env=jsdom",
    "cosmos": "NODE_PATH=src cosmos"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "react-cosmos": "^3.2.0",
    "react-cosmos-redux-proxy": "^3.1.1",
    "react-cosmos-router-proxy": "^3.1.1",
    "react-cosmos-test": "^3.2.0",
    "react-cosmos-xhr-proxy": "^3.1.1",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "sinon": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": ["web.js", "mjs", "js", "json", "web.jsx", "jsx", "node"]
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          [
            "emotion",
            {
              "hoist": true
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          [
            "emotion",
            {
              "sourceMap": true,
              "autoLabel": true
            }
          ]
        ]
      }
    },
    "presets": ["react-app"],
    "plugins": ["emotion"]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
